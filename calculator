class calculator:
    def add(self,a,b):
        print("addition:",a+b)
    def sub(self,a,b):
        print("subtration",a-b)
    def mul(self,a,b):
        print("multiplication",a*b)
    def div(self,a,b):
        print("division:",a/b)
num1=calculator()

a=int(input("enter vlaue a:"))
b=int(input("enter value b:"))
print()
num1.add(a,b)
num1.sub(a,b)
num1.mul(a,b)
num1.div(a,b)
